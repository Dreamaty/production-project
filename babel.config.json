{
	"presets": [
		"@babel/preset-env",
		["@babel/preset-typescript", {
			"isTsx": true
		}],
		[
			"@babel/preset-react", 
			{ 
				"runtime": "automatic" 
			}
		]
	],
	"plugins": [
		"i18next-extract",
		"@babel/plugin-transform-class-properties",
		"@babel/plugin-transform-nullish-coalescing-operator",
		"@babel/plugin-transform-optional-chaining",
		"@babel/plugin-transform-runtime"
	]
}
